Nome: Novo Ninho
Método: POST
URL: <DNS>/catalogacao/v1/addNewNest

Request:
{
	"cityId" : 123,
	"name" : "nest 1",
	"collectorId" : 3,
	"vegetation" : "vegetation",
	"beginPoint" : {
		"latitude" : -21.0000,
		"longitude" : -47.0000
	},
	"endingPoint" : {
		"latitude" : -21.0011,
		"longitude" : -47.0011
	},
}

Response: 
{
	"nestId" : 123
	"message" : "salvo | erro"
}

