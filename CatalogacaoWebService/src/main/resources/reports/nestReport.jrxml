	<queryString language="SQL">
		<![CDATA[SELECT 
    V.ID AS ATUALIZACAO,
    V.nest_nest_id AS NINHO,
    DATE_FORMAT(V.collection_date, '%d/%m/%y') AS DATA,
    CONCAT('Lat: ',
            INITIAL_COORDINATE.latitude,
            '\r\nLong: ',
            INITIAL_COORDINATE.longitude) AS PONTO_INICIAL,
    CONCAT('Lat: ', FINAL_COORDINATE.latitude,
            '\r\nLong: ',
            FINAL_COORDINATE.longitude) AS PONTO_FINAL,
	V.notes as OBSERVACOES,
    IFNULL(GROUP_CONCAT(CONCAT($P{IMAGE_PATH}, P.FILEPATH)
        SEPARATOR '; '), '-') AS FOTOS,
    IFNULL(GROUP_CONCAT(A.NAME
        SEPARATOR '; '), '-') AS AMOSTRAS
FROM
    DATA_UPDATE_VISIT V
        LEFT JOIN
    DATA_UPDATE_VISIT_PHOTOS I ON I.DATA_UPDATE_VISIT_ID = V.ID
        LEFT JOIN
    PHOTO P ON P.ID = I.PHOTOS_ID
        LEFT JOIN
    COORDINATE INITIAL_COORDINATE ON (INITIAL_COORDINATE.id = V.new_begining_point_id)
        LEFT JOIN
    COORDINATE FINAL_COORDINATE ON (FINAL_COORDINATE.id = V.new_ending_point_id)
		LEFT JOIN 
	ANT A ON A.VISIT_ID = V.ID 
GROUP BY V.ID ORDER BY V.NEST_NEST_ID;]]>
	</queryString>
